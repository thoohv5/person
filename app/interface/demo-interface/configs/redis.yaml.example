# Redis配置
redis:
  # 网络协议
  network: tcp
  # 地址
  addr: 127.0.0.1:6379
  # 连接超时
  connectiontimeout: 10
  # 读取超时
  readtimeout: 10
  # 写入超时
  writetimeout: 10
  # 密码
  password: password
  # 数据库
  db: 0
  # 最大空闲连接数
  maxidle: 10
  # 最大活跃连接数
  maxactive: 10
  # 链接测试
  testonborrow: true
  # 空闲连接超时
  idletimeout: 10
  # 等待超时
  wait: true